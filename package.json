        "leaflet": "^1.9.4",
        "lint-staged": "^16.2.0",
        "lucide-react": "0.474.0",
        "next": "15.5.3",
        "next-themes": "0.4.6",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-leaflet": "^5.0.0",
